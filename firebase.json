{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "source": "**",
    "header": [{
      "key": "Cache-Control",
      "value": "no-cache"
    }],
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }]
  }
}